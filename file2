    "Col_Unic_itens = dict()\n",
    "for n in range(1,Col_lenght):\n",
    "    Col_Unic_itens[Colunas[n]] = set()\n",
    "\n",
    "for data in DF_Poducts_SBU:\n",
    "    for n in range(1,Col_lenght):\n",
    "        Col_Unic_itens[Single_sets[n]].add(data[n])\n",
