            else:
                messagebox.showerror(title='Erro de Sele√ß√£o', message='Por favor, selecione\n   uma das op√ß√µes' )
            pass
        
        #criando bot√£o para gera√ß√£o de PDF de etiquetas
        self.B1 = CTkButton(self.tabview.tab("Etiquetas"), text = "Gerar Etiquetas",command = Gerar_PDF, 
                                font=("arial bold",18),fg_color="#ba0606")
        self.B1.pack(pady = 25)
        
        #criando Label de Aviso
        self.Label_aviso = ctk.CTkLabel(self.tabview.tab("Etiquetas"), text="* No momento da impress√£o selecione o tamanho Carta",
                                            font=("arial",12))
        self.Label_aviso.pack()
        
        
        ##### CRIANDO ABA DE "Controle de Dados" #################################################################
        
        # criando calend√°rio para sele√ß√£o de validade
        self.Calen_Validade = Calendar(self.tabview.tab("Controle de Dados"),selectmode = 'day', 
